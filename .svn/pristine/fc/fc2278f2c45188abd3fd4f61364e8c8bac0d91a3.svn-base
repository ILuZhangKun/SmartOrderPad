html, body {
    margin: 0;
    padding: 0;
}
ul {
    list-style-type: none;
    padding: 0;
    margin: 0;
}
.header {
    width: 100%;
    height: 45px;
    background-color: #09f;
    position: fixed;
    top: 0;
    left: 0;
    font-size: 14px;
}
.menu {
    width: 400px;
    height: 35px;
    margin: 0 auto;
}
.menu ul {
    width: 100%;
    height: 100%;
    margin-top: 10px;
}
.menu li {
    width: 90px;
    height: 35px;
    line-height: 35px;
    float: left;
    margin-right: 5px;
    text-align: center;
    color: #fff;
    position: relative;
    cursor: pointer;
}
.active {
    background-color: #fff;
    color: #000!important;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
}
.active .cricle {
    width: 10px;
    height: 10px;
    background-color: #fff;
    position: relative;
}
.active .left .cricle div {
    width: 20px;
    height: 20px;
    position: absolute;
    right: 0;
    bottom: 0;
    border-radius: 10px;
    background-color: #09f;
}
.active .left {
    position: absolute;
    bottom: 0;
    left: -10px;
}
.active .right {
    position: absolute;
    bottom: 0;
    right: -10px;
}
.active .right .cricle div {
    width: 20px;
    height: 20px;
    position: absolute;
    left: 0;
    bottom: 0;
    border-radius: 10px;
    background-color: #09f;
}
.exit {
    position: absolute;
    right: 20px;
    width: 50px;
    height: 45px;
    line-height: 45px;
    text-align: center;
    top: 0;
    right: 20px;
    color: #fff;
    cursor: pointer;
}
